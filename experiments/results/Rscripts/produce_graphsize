### we don't record size of original map very well; use HPA numbers instead ###
avgnodes <- 4469
avgedges <- 16420

pdf(file="graphsize.pdf", width=10, height=6)
par(mfrow=c(1,2))
plot(gs_edges$"r0gs_cs10hq$edges", gs_edges$"r0gs_cs10hq$edges", type="l", main="Edges in Graph (SO=10%, AQ=High)", xlab="# of  edges", ylab="# of  edges")
lines(gs_edges$"r0gs_cs10hq$edges", gs_edges$"r10gs_cs10hq$absedges", col="red", lty=2)
lines(gs_edges$"r0gs_cs10hq$edges", gs_edges$"r10gs_cs15hq$absedges", col="blue", lty=3)
lines(gs_edges$"r0gs_cs10hq$edges", gs_edges$"r10gs_cs20hq$absedges", col="orange", lty=4)
points(gs_edges$"r0gs_cs10hq$edges", gs_edges$"r0gs_cs10hq$edges", pch=1)
legend("topleft", c("No abstraction", "CS = 10", "CS = 15", "CS = 20"), lty=c(1,2,3,4), col=c("black", "red", "blue", "orange"))

plot(gs_nodes$"r0gs_cs10hq$nodes", gs_nodes$"r0gs_cs10hq$nodes", type="l", main="Nodes in Graph (SO=10%, AQ=High)", xlab="# of  nodes", ylab="# of  nodes")
lines(gs_nodes$"r0gs_cs10hq$nodes", gs_nodes$"r10gs_cs10hq$absnodes", col="red", lty=2)
lines(gs_nodes$"r0gs_cs10hq$nodes", gs_nodes$"r10gs_cs15hq$absnodes", col="blue", lty=3)
lines(gs_nodes$"r0gs_cs10hq$nodes", gs_nodes$"r10gs_cs20hq$absnodes", col="orange", lty=4)
points(gs_nodes$"r0gs_cs10hq$nodes", gs_nodes$"r0gs_cs10hq$nodes", pch=1)
legend("topleft", c("No abstraction", "CS = 10", "CS = 15", "CS = 20"), lty=c(1,2,3,4), col=c("black", "red", "blue", "orange"))
dev.off()

gsnodes_table <- as.data.frame(mean(r0gs_cs10hq$absnodes) / avgnodes)
gsnodes_table <- cbind(gsnodes_table, mean(r5gs_cs10hq$absnodes) / avgnodes)
gsnodes_table <- cbind(gsnodes_table, mean(r10gs_cs10hq$absnodes) / avgnodes)
gsnrow <- mean(r0gs_cs15lq$absnodes) / avgnodes
gsnrow <- c(gsnrow, mean(r5gs_cs15lq$absnodes) / avgnodes)
gsnrow <- c(gsnrow, mean(r10gs_cs15lq$absnodes) / avgnodes)
gsnodes_table <- rbind(gsnodes_table, gsnrow)
gsnrow <- c()
gsnrow <- mean(r0gs_cs15hq$absnodes) / avgnodes
gsnrow <- c(gsnrow, mean(r5gs_cs15hq$absnodes) / avgnodes)
gsnrow <- c(gsnrow, mean(r10gs_cs15hq$absnodes) / avgnodes)
gsnodes_table <- rbind(gsnodes_table, gsnrow)
gsnrow <- c()
gsnrow <- mean(r0gs_cs15lq$absnodes) / avgnodes
gsnrow <- c(gsnrow, mean(r5gs_cs15lq$absnodes) / avgnodes)
gsnrow <- c(gsnrow, mean(r10gs_cs15lq$absnodes) / avgnodes)
gsnodes_table <- rbind(gsnodes_table, gsnrow)
gsnrow <- c()
gsnrow <- mean(r0gs_cs20hq$absnodes) / avgnodes
gsnrow <- c(gsnrow, mean(r5gs_cs20hq$absnodes) / avgnodes)
gsnrow <- c(gsnrow, mean(r10gs_cs20hq$absnodes) / avgnodes)
gsnodes_table <- rbind(gsnodes_table, gsnrow)
gsnrow <- c()
gsnrow <- mean(r0gs_cs20lq$absnodes) / avgnodes
gsnrow <- c(gsnrow, mean(r5gs_cs20lq$absnodes) / avgnodes)
gsnrow <- c(gsnrow, mean(r10gs_cs20lq$absnodes) / avgnodes)
gsnodes_table <- rbind(gsnodes_table, gsnrow)
gsnodes_table <- gsnodes_table*100
dimnames(gsnodes_table) <- list(c("CS10 HQ", "CS10 LQ", "CS15 HQ", "CS15 LQ", "CS20 HQ", "CS20 LQ"), c("0% SO", "5% SO", "10% SO"))

library(xtable)
gsnoutput <- xtable(gsnodes_table)

