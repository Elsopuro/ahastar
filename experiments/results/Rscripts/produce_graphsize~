gs_edges <- cbind(gs_edges, c10gs_cs15lq$absedges)
gs_edges <- cbind(gs_edges, c10gs_cs20hq$absedges)
gs_edges <- cbind(gs_edges, c10gs_cs20lq$absedges)
gs_edges <- gs_edges[order(gs_edges$"c0gs_cs10hq$edges"),]

gs_nodes <- as.data.frame(c0gs_cs10hq$nodes)
gs_nodes <- cbind(gs_nodes, maporder)
gs_nodes <- cbind(gs_nodes, c0gs_cs10hq$absnodes)
gs_nodes <- cbind(gs_nodes, c0gs_cs10lq$absnodes)
