gs_edges <- as.data.frame(c0gs_cs10hq$edges)
gs_edges <- cbind(gs_edges, maporder)
gs_edges <- cbind(gs_edges, c0gs_cs10hq$absedges)
gs_edges <- cbind(gs_edges, c0gs_cs10lq$absedges)
