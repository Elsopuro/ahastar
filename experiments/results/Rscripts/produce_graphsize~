plot(gs_edges$"c0gs_cs10hq$edges", gs_edges$"c0gs_cs10hq$edges", type="l", main="Edges in Graph (SO=10%, AQ=High)", xlab="# non-abstract nodes", ylab="# non-abstract nodes")
lines(gs_edges$"c0gs_cs10hq$edges", gs_edges$"c10gs_cs10hq$absedges", col="red", lty=2)
lines(gs_edges$"c0gs_cs10hq$edges", gs_edges$"c10gs_cs15hq$absedges", col="blue", lty=3)
lines(gs_edges$"c0gs_cs10hq$edges", gs_edges$"c10gs_cs20hq$absedges", col="orange", lty=4)
points(gs_edges$"c0gs_cs10hq$edges", gs_edges$"c0gs_cs10hq$edges", pch=1)
legend("topleft", c("No abstraction", "CS = 10", "CS = 15", "CS = 20"), lty=c(1,2,3,4), col=c("black", "red", "blue", "orange"))

plot(gs_nodes$"c0gs_cs10hq$nodes", gs_nodes$"c0gs_cs10hq$nodes", type="l", main="Nodes in Graph (SO=10%, AQ=High)", xlab="# non-abstract nodes", ylab="# non-abstract nodes")
lines(gs_nodes$"c0gs_cs10hq$nodes", gs_nodes$"c10gs_cs10hq$absnodes", col="red", lty=2)
lines(gs_nodes$"c0gs_cs10hq$nodes", gs_nodes$"c10gs_cs15hq$absnodes", col="blue", lty=3)
